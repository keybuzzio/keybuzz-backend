{
  "name": "keybuzz-backend",
  "version": "1.0.0",
  "description": "KeyBuzz Backend v3 - Fastify + TypeScript",
  "main": "dist/main.js",
  "license": "ISC",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpileOnly src/main.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/main.js",
    "lint": "eslint src --ext .ts",
    "test": "echo \"No tests defined yet\"",
    "prisma:migrate": "prisma migrate dev --name init_core_schema",
    "prisma:seed": "prisma db seed",
    "worker:amazon": "node dist/workers/amazonPollingWorker.js",
    "worker:amazon:once": "node dist/workers/amazonPollingWorker.js --once"
  },
  "dependencies": {
    "@fastify/cors": "^11.2.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@prisma/client": "^6.3.0",
    "bcrypt": "^5.1.1",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.2",
    "fastify-jwt": "^4.1.3",
    "fastify-plugin": "^5.1.0",
    "node-fetch": "^2.7.0",
    "pg": "^8.16.3",
    "prisma": "^6.3.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.0",
    "@types/node-fetch": "^2.6.13",
    "@types/pg": "^8.10.1",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "eslint": "^9.39.1",
    "prettier": "^3.7.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
